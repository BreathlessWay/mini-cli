{
    "name": "mini",
    "version": "0.0.1",
    "description": "mini program cli",
    "main": "bin/mini",
    "bin": {
        "mini": "bin/mini"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "release": "standard-version",
        "link": "npm link --force",
        "build": "npm run clean && tsc",
        "dev": "tsc --watch",
        "clean": "rimraf dist"
    },
    "_moduleAliases": {
        "@": "dist"
    },
    "keywords": [
        "miniprogram",
        "commander"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "author": "BreathlessWay",
    "license": "ISC",
    "dependencies": {
        "commander": "^6.1.0",
        "git-clone": "^0.1.0",
        "inquirer": "^7.3.3",
        "mem-fs": "^1.2.0",
        "mem-fs-editor": "^7.0.1",
        "module-alias": "^2.2.2",
        "shelljs": "^0.8.4",
        "tracer": "^1.1.3"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@types/inquirer": "^7.3.1",
        "@types/rimraf": "^3.0.0",
        "@types/shelljs": "^0.8.8",
        "del": "^6.0.0",
        "husky": "^4.3.0",
        "prettier": "^2.1.2",
        "pretty-quick": "^3.0.2",
        "rimraf": "^3.0.2",
        "standard-version": "^9.0.0",
        "typescript": "^4.0.3"
    }
}
